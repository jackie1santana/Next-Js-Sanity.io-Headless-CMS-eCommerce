#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/max1solenov/Desktop/docker_apps/sanity_clothing/redis/node_modules/mime/node_modules:/home/max1solenov/Desktop/docker_apps/sanity_clothing/redis/node_modules:/home/max1solenov/Desktop/docker_apps/sanity_clothing/node_modules:/home/max1solenov/Desktop/docker_apps/node_modules:/home/max1solenov/Desktop/node_modules:/home/max1solenov/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
