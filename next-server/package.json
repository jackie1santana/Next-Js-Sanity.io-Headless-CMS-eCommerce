{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next-server": "next dev",
    "build": "next build",
    "start": "next start",
    "pretty": "npx prettier --write .",
    "test": "jest"
  },
  "keywords": [
    "next.js",
    "async",
    "error handling",
    "NODE_ENV",
    "env",
    "typescript"
  ],
  "dependencies": {
    "@sls-next/serverless-component": "^1.19.0",
    "@types/aws-lambda": "^8.10.76",
    "dotenv": "^9.0.0",
    "i": "^0.3.6",
    "next": "11.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.32.11"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.31.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/react": "^17.0.4",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.13",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
}
}
